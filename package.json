  "scripts": {
    "test": "jest",
    "prepare": "husky install",
    "eslint-fix": "eslint --ext .ts,.tsx,.js,.jsx,json --fix ./src",
    "static-check": "tsc",
    "prettier-style": "prettier --write ./src",
    "server": "cross-env NODE_ENV=development rollup -c build/rollup.config.js -w",
