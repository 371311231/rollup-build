  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "./src/*.{ts,tsx,js,jsx,json}": [
      "prettier --write ./src",
      "eslint --ext .ts,.tsx,.js,.jsx,json --fix ./src"
